{
    "name": "@slimio/manifest",
    "version": "0.6.0",
    "description": "This package was created to manage manifest for the SlimIO Project.",
    "main": "index.js",
    "scripts": {
        "prepublishOnly": "pkg-ok",
        "test": "cross-env psp && ava --verbose",
        "doc": "jsdoc -c ./jsdoc.json -r -R ./README.md -P ./package.json --verbose",
        "coverage": "nyc npm test",
        "report": "nyc report --reporter=html"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SlimIO/Manifester.git"
    },
    "keywords": [
        "SlimIO",
        "monitoring",
        "manifest"
    ],
    "author": "SlimIO",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/SlimIO/Manifester/issues"
    },
    "homepage": "https://github.com/SlimIO/Manifester#readme",
    "dependencies": {
        "@iarna/toml": "^2.2.3",
        "@slimio/is": "^1.5.0",
        "lodash.clonedeep": "^4.5.0",
        "ow": "^0.13.2",
        "semver": "^6.0.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^8.0.0",
        "@commitlint/config-conventional": "^8.0.0",
        "@escommunity/minami": "^1.0.0",
        "@slimio/eslint-config": "^2.0.4",
        "@slimio/psp": "^0.4.0",
        "ava": "^2.0.0",
        "cross-env": "^5.2.0",
        "eslint": "^5.16.0",
        "husky": "^2.2.0",
        "jsdoc": "^3.5.5",
        "nyc": "^14.1.0",
        "pkg-ok": "^2.3.1"
    },
    "engines": {
        "node": ">=10"
    },
    "husky": {
        "hooks": {
            "pre-push": "cross-env eslint index.js src/assert.js && npm test",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "nyc": {
        "check-coverage": true,
        "per-file": false,
        "lines": 98,
        "statements": 98,
        "functions": 98,
        "branches": 90
    }
}
